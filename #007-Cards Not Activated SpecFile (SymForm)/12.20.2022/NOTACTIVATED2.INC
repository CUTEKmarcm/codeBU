[Populate FORMDATA array]
PROCEDURE ASSIGNFIELDVARIABLES
  TOTALINDEX    = 0
  FIELDMAX      = 008
  DATACOUNT(01) = 008
  FORMDATA(001)=FORMAT("99/99/9999",SYSTEMDATE)
  FORMDATA(002)=ACCOUNT:PAYEELINE:1
  FORMDATA(003)=ACCOUNT:PAYEELINE:2
  FORMDATA(004)=ACCOUNT:PAYEELINE:3
  FORMDATA(005)=ACCOUNT:PAYEELINE:4
  FORMDATA(006)=SEGMENT(SELECTEDCARDNUMBER,LENGTH(SELECTEDCARDNUMBER)-3,LENGTH(SELECTEDCARDNUMBER))
  FORMDATA(007)=FORMAT("99/99/99",SELECTEDCARDISSUED)
  FORMDATA(008)=FORMAT("99/99/99",DATEOFFSET(SYSTEMDATE,0,31))+"."

  FOR FORMLOOP=1 TO FIELDMAX
   DO
    TEMPCHAR=FORMDATA(FORMLOOP)
    CALL SLASHOUTPUT
    FORMDATA(FORMLOOP)=TEMPCHAR
   END
END

[Assign FORMDATA back for FM]
PROCEDURE UPDATEFIELDVARIABLES
END
