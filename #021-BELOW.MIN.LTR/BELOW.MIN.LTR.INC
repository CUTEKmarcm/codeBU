[Populate FORMDATA array]
PROCEDURE ASSIGNFIELDVARIABLES
  TOTALINDEX    = 0
  FIELDMAX      = 010
  DATACOUNT(01) = 008
  FORMDATA(001)=LONGDATE
  FORMDATA(002)=ADDRESS1
  FORMDATA(003)=ADDRESS2
  FORMDATA(004)=ADDRESS3
  FORMDATA(005)=ADDRESS4
  FORMDATA(006)=ACCTNO
  FORMDATA(007)=NEGAMT
  FORMDATA(008)=NEEDED

  FOR FORMLOOP=1 TO FIELDMAX
   DO
    TEMPCHAR=FORMDATA(FORMLOOP)
    CALL SLASHOUTPUT
    FORMDATA(FORMLOOP)=TEMPCHAR
   END
END

[Assign FORMDATA back for FM]
PROCEDURE UPDATEFIELDVARIABLES
END
