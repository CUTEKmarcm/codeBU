[marcm.009]
TARGET=SHARE

SELECT
 SHARE:CLOSEDATE='--/--/--' AND
 SHARE:TYPE=0
END

SORT
 SHARE:BRANCH SUBTOTAL = "BRANCH"
END

PRINT TITLE="Report"
 SUPPRESSNEWLINE
END

[
TARGET=ACCOUNT

DEFINE
 PRIMESHARECOUNT=NUMBER
END

SELECT
 ACCOUNT:CLOSEDATE='--/--/--'
END

PRINT TITLE="Report"
 HEADER=""
 FOR EACH SHARE WITH SHARE:TYPE=0 AND SHARE:CLOSEDATE='--/--/--'
  DO
   PRIMESHARECOUNT=PRIMESHARECOUNT+1
  END
END

TOTAL
 COL=010 PRIMESHARECOUNT
END
] 
