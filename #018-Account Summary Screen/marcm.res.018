[marcm.res.018]
TARGET=ACCOUNT

DEFINE
 MATCHCOUNT=2
 SSNARRAY=CHARACTER(9) ARRAY(5)
 MATCH1=NUMBER
 MATCH2=NUMBER
 MATCH3=NUMBER
 MATCH4=NUMBER
 MATCH5=NUMBER
 Z=NUMBER
END

SELECT
 ACCOUNT:CLOSEDATE='--/--/--' AND
 TRACKING:TYPE=31
END

PRINT TITLE="Report"
 HEADER=""
 MATCH1=0
 MATCH2=0
 MATCH3=0
 MATCH4=0
 MATCH5=0
 FOR EACH TRACKING WITH TRACKING:TYPE=31
  DO
   SSNARRAY(1)=TRACKING:USERCHAR1
   SSNARRAY(2)=TRACKING:USERCHAR2
   SSNARRAY(3)=TRACKING:USERCHAR3
   SSNARRAY(4)=TRACKING:USERCHAR4
   SSNARRAY(5)=TRACKING:USERCHAR5
  END
 FOR Z=1 TO 5
  DO
   IF SSNARRAY(1)<>"" AND SSNARRAY(Z) = SSNARRAY(1) THEN
    DO
     MATCH1=MATCH1+1
    END
   IF SSNARRAY(2)<>"" AND SSNARRAY(Z) = SSNARRAY(2) THEN
    DO
     MATCH2=MATCH2+1
    END     
   IF SSNARRAY(3)<>"" AND SSNARRAY(Z) = SSNARRAY(3) THEN
    DO
     MATCH3=MATCH3+1
    END
   IF SSNARRAY(4)<>"" AND SSNARRAY(Z) = SSNARRAY(4) THEN
    DO
     MATCH4=MATCH4+1
    END
   IF SSNARRAY(5)<>"" AND SSNARRAY(Z) = SSNARRAY(5) THEN
    DO
     MATCH5=MATCH5+1
    END           
  END
 IF MATCH1>MATCHCOUNT OR MATCH2>MATCHCOUNT OR MATCH3>MATCHCOUNT OR MATCH4>MATCHCOUNT OR MATCH5>MATCHCOUNT THEN
  DO
   COL=001 ACCOUNT:NUMBER
  END 
END
  
