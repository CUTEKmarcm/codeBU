[Populate FORMDATA array]
PROCEDURE ASSIGNFIELDVARIABLES
  TOTALINDEX    = 0
  FIELDMAX      = 018
  DATACOUNT(01) = 018
  
  SPELLDATE=SYSTEMDATE
  CALL DATESPELL
  FORMDATA(001)=SPELLCHAR   
  FORMDATA(002)=PRIMENAME
  FORMDATA(003)=PRIMESTREET
  FORMDATA(004)=PRIMECITY+", "+PRIMESTATE+"  "+PRIMEZIP
  FORMDATA(005)=PRIMENAME+","
  FORMDATA(006)=CAPITALIZE(COLCODEDESCRIPTION)
  FORMDATA(007)=LAMOUNT
  FORMDATA(008)=LRATE
  FORMDATA(009)=LTERM+" Months"
  FORMDATA(010)=LPAYMENT
  FORMDATA(011)=LFIRSTDUEDATE
  TEMPCHAR=ACCOUNT:NUMBER
  CALL REMOVELEADINGZEROS
  FORMDATA(012)=TEMPCHAR
  FORMDATA(013)=SELECTEDLID
  FORMDATA(014)="314977175"
  FORMDATA(015)="Austin Telco Federal Credit Union"
  FORMDATA(016)="11149 Research Blvd. Suite 300"
  FORMDATA(017)="Austin, TX 78759"
  FORMDATA(018)="512/302-5555 ext. 7194"

  FOR FORMLOOP=1 TO FIELDMAX
   DO
    TEMPCHAR=FORMDATA(FORMLOOP)
    CALL SLASHOUTPUT
    FORMDATA(FORMLOOP)=TEMPCHAR
   END
END

